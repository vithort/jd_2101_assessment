package com.primeit;

import java.util.Arrays;

public class Student {

	// Write a Student Class and call all these methods mentioned above and Print
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		MathCourse mathCourse = new MathCourse();
		
		int[] arr = { 13, 7, 6, 45, 21, 9, 101, 102 };
		int[] arrResult = {};
		arrResult = mathCourse.sortArray(arr);
		System.out.println("Math Course[] : " + Arrays.toString(arrResult));

		String string = "9";
		int resultado = (int) (Math.sqrt(Integer.parseInt(string)));
		System.out.println(resultado);

		
		
		for (int i = 1; i <= 10; i++) {
			System.out.println(fib(i) + ", ");
		}
	}

	public static long fib(int n) {
		if (n <= 1) {
			return n;
		} else {
			return fib(n - 1) + fib(n - 2);
		}
	}
	
	private static void print(Object obj) {
		
	}

}
